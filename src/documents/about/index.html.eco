---
name: about
title: About
description: About the WebComponents.org.
layout: about
---

<% for item in @fixOrder(@getCollection("about").toJSON()): %>
    <% if item.link: %>
    <section class="card card-about" itemscope itemtype="http://schema.org/Product">
        <h3 itemprop="name"><%= item.title %></h3>

        <div class="description" itemprop="description">
            <%- item.content %>
        </div>

    </section>
    <% end %>
<% end %>
